{
  "name": "@boxraiser/visual-editor",
  "version": "0.0.1",
  "type": "module",
  "main": "VisualEditor.js",
  "types": "./VisualEditor.d.ts",
  "scripts": {
    "dev": "concurrently \"vite\" \"php -S 0.0.0.0:8000 -ddisplay_errors=1 -t server/\"",
    "build": "tsc && rm -rf dist && node build.js && npm run declaration && cp package.json dist/package.json",
    "serve": "vite preview",
    "format": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' --write",
    "declaration": "tsc --declaration --emitDeclarationOnly --jsx react-jsx --esModuleInterop --target es2020 -p tsconfig.json --noEmit false && tsc-alias",
    "test": "jest"
  },
  "dependencies": {
    "@dnd-kit/core": "^3.1.1",
    "@dnd-kit/modifiers": "^3.0.0",
    "@dnd-kit/sortable": "^4.0.0",
    "@dnd-kit/utilities": "^3.0.1",
    "@emotion/cache": "^11.7.1",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@emotion/weak-memoize": "^0.2.5",
    "@popperjs/core": "^2.9.2",
    "@radix-ui/react-dialog": "^0.1.1",
    "@radix-ui/react-popover": "^0.1.1",
    "@radix-ui/react-slider": "^0.1.1",
    "@radix-ui/react-tabs": "^0.1.1",
    "@reach/dialog": "^0.16.2",
    "@reach/tabs": "^0.16.4",
    "@tippyjs/react": "^4.2.5",
    "clsx": "^1.1.1",
    "framesync": "^6.1.0",
    "quill": "^1.3.7",
    "react-flip-toolkit": "^7.0.13",
    "react-popper": "^2.2.5",
    "react-use": "^17.2.4",
    "zustand": "^3.5.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/quill": "^2.0.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.1.0",
    "concurrently": "^6.5.1",
    "esbuild": "^0.14.8",
    "esbuild-node-externals": "^1.4.1",
    "esbuild-style-plugin": "^1.1.2",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "retypeapp": "^1.11.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.45.1",
    "ts-jest": "^27.0.3",
    "tsc-alias": "^1.5.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-jest": "^0.0.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
